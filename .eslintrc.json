{
  "extends": ["airbnb", "prettier"],
  "plugins": ["jest", "prettier"],
  "env": {
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "sourceType": "module",
    "allowImportExportEverywhere": true
  },
  "rules": {
    "import/prefer-default-export": "off",
    "prettier/prettier": ["error"]
  }
}
